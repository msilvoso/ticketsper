set xdata time
set format x "%m-%d"
set timefmt "%m-%d"
set xrange [XFROMX:XTOX]
set yrange [0:100]
set style fill solid 1.00 border 0
set term png
set output "/tmp/ticketsplot.png"
plot "/tmp/perweek" using 1:2:xtic(1) title "Tickets per week" with boxes, "/tmp/perday" using 1:2 title "Tickets per day" with lines lt 3 lc rgb 'red'